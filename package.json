{
  "name": "okenv",
  "dependencies": {
    "@types/figlet": "^1.5.5",
    "@types/js-yaml": "^4.0.5",
    "chalk": "4.1.2",
    "commander": "^9.4.1",
    "conf": "^10.2.0",
    "docker-cli-js": "^2.9.0",
    "dotenv": "^16.0.3",
    "figlet": "^1.5.2",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.14.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "main": "./src/index.js",
  "bin": {
    "okenv": "./src/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && onebox",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  }
}
